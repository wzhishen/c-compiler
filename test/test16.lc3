	;; LINE 1: int fun (int a, int b, int c) {
	;; prologue for fun
fun	ADD SP, SP, -1 ; push return value
	ADD SP, SP, -1 ; push return address
	STR R7, SP, 0
	ADD SP, SP, -1 ; push FP
	STR FP, SP, 0
	ADD FP, SP, -1 ; set FP
	;; prologue ends

	;; LINE 2: return a + b + c + 10;
	LDR R0, FP, 4 ; read a
	LDR R1, FP, 5 ; read b
	ADD R0, R0, R1
	LDR R1, FP, 6 ; read c
	ADD R0, R0, R1
	AND R1, R1, 0
	ADD R1, R1, 10
	ADD R0, R0, R1
	STR R0, FP, 3 ; write return value

	;; LINE 3: }
	;; epilogue starts
	LDR FP, SP, 0 ; restore old FP
	ADD SP, SP, 1
	LDR R7, SP, 0 ; restore old PC
	ADD SP, SP, 1
	ADD SP, SP, 1 ; pop return value
	ADD SP, SP, 1 ; pop param
	ADD SP, SP, 1 ; pop param
	ADD SP, SP, 1 ; pop param
	RET
	;; epilogue ends
