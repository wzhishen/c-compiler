	;; LINE 1: int fun (int a) {
	;; prologue for fun
fun	ADD SP, SP, -1 ; push return value
	ADD SP, SP, -1 ; push return address
	STR R7, SP, 0
	ADD SP, SP, -1 ; push FP
	STR FP, SP, 0
	ADD FP, SP, -1 ; set FP
	;; prologue ends

	;; LINE 2: return a;
	LDR R0, FP, 4 ; read a
	STR R0, FP, 3 ; write return value

	;; LINE 3: }
	;; epilogue starts
	LDR FP, SP, 0 ; restore old FP
	ADD SP, SP, 1
	LDR R7, SP, 0 ; restore old PC
	ADD SP, SP, 1
	ADD SP, SP, 1 ; pop return value
	ADD SP, SP, 1 ; pop param
	RET
	;; epilogue ends
